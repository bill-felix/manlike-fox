rdate <- function(n, start, end) {
st <- as.POSIXct(as.Date(start))
et <- as.POSIXct(as.Date(end))
dt <- as.numeric(difftime(et,st,unit="sec"))
ev <- runif(n, min = 1.1, max = dt)
rt <- st + ev
}
